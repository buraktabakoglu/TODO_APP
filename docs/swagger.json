                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    }
                }
            },
            "patch": {
                "produces": [
                    "application/json"
                ],
                "summary": "patch a users item by ID",
                "operationId": "patch-todo-by-id",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "users ID",
                        "name": "id",
                        "in": "path",
