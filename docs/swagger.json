        "/api/users": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "get a users item by ID",
                "operationId": "get-users-by-id",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "users ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    }
