        "/api/signin": {
            "post": {
                "description": "Get token for authenticated user",
                "produces": [
                    "application/json"
                ],
                "summary": "Sign in user and get token",
                "parameters": [
                    {
                        "type": "string",
                        "description": "User email",
                        "name": "email",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "User password",
                        "name": "password",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/todos": {
            "get": {
                "security": [
                    {
                        "JWT": []
                    }
                ],
                "description": "Get All TODOs",
                "produces": [
                    "application/json"
                ],
                "summary": "Get Todos",
                "responses": {
                    "200": {
